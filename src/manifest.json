{
  "name": "Firebase testing",
  "version": "0.1",
  "description": "Firebase..",
  "permissions": ["activeTab", "tabs"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "firebase.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["app.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 3
}
